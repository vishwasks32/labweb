<span id="cb1-9"><a href="#cb1-9" aria-hidden="true"></a>            <span class="co">// let n1 = Number(document.getElementById(&quot;num1&quot;).value);</span></span>
<span id="cb1-10"><a href="#cb1-10" aria-hidden="true"></a>            <span class="co">// let n2 = Number(document.getElementById(&quot;num2&quot;).value);</span></span>
<span id="cb1-11"><a href="#cb1-11" aria-hidden="true"></a></span>
<span id="cb1-12"><a href="#cb1-12" aria-hidden="true"></a>            <span class="kw">let</span> n1 <span class="op">=</span> <span class="bu">Number</span>(<span class="bu">document</span><span class="op">.</span><span class="fu">querySelector</span>(<span class="st">&quot;#num1&quot;</span>)<span class="op">.</span><span class="at">value</span>)<span class="op">;</span></span>
