<span id="cb1-17"><a href="#cb1-17" aria-hidden="true"></a>                <span class="fu">alert</span>(<span class="st">&quot;The number entered is not a number&quot;</span>)<span class="op">;</span></span>
<span id="cb1-18"><a href="#cb1-18" aria-hidden="true"></a>            } <span class="cf">else</span> {</span>
<span id="cb1-19"><a href="#cb1-19" aria-hidden="true"></a>                <span class="fu">alert</span>(<span class="vs">`</span><span class="sc">${</span>n1<span class="sc">}</span><span class="vs"> + </span><span class="sc">${</span>n2<span class="sc">}</span><span class="vs"> = </span><span class="sc">${</span>n1<span class="op">+</span>n2<span class="sc">}</span><span class="vs">`</span>)<span class="op">;</span></span>
<span id="cb1-20"><a href="#cb1-20" aria-hidden="true"></a>            }</span>
<span id="cb1-21"><a href="#cb1-21" aria-hidden="true"></a>        }</span>
